{
  "name": "mow-e-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint src && eslint __tests__ && prettier --check --ignore-unknown src && prettier --check --ignore-unknown __tests__",
    "lint:fix": "eslint --fix src && eslint --fix __tests__ && prettier --write --ignore-unknown src && prettier --write --ignore-unknown __tests__",
    "lint:staged": "lint-staged",
    "start": "react-native start",
    "test": "jest --verbose --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "6.5.7",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/stack": "6.3.16",
    "i18next": "22.4.14",
    "react": "18.2.0",
    "react-i18next": "12.2.0",
    "react-native": "0.71.6",
    "react-native-bluetooth-classic": "1.60.0-rc.26",
    "react-native-gesture-handler": "2.9.0",
    "react-native-reanimated": "3.0.2",
    "react-native-safe-area-context": "4.5.1",
    "react-native-screens": "3.20.0",
    "react-native-svg": "13.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/runtime": "7.21.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@testing-library/react-native": "12.0.1",
    "@tsconfig/react-native": "2.0.3",
    "@types/jest": "29.5.0",
    "@types/react": "18.0.34",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.5.0",
    "eslint": "8.38.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "2.8.7",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|@react-navigation)"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "__tests__/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
